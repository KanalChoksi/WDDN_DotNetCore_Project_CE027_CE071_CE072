@model IEnumerable<Texi_Booking.Models.City>


<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">City List</h2>
        </div>
        <div class="col-6 text-right">
            <a asp-controller="Admin" asp-action="AddCity" class="btn btn-primary">Add New City</a>

        </div>

    </div>

    <br /><br />


    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>@Html.DisplayNameFor(model => model.Cities)</th>

            </tr>
        </thead>
        <tbody>
            @foreach(var obj in Model)
            {
                <tr>

                    <td width="30%">@obj.Cities</td>

                </tr>
            }
        </tbody>
    </table>


</div>

